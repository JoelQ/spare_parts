---
title: Welcome to Middleman
---

<% data.snippets.each do |category| %>
  <h2><%= category[:category] %></h2>
    <% category[:snippets].each do |snippet| %>
      <%= snippet[:title] %>
        <div class="example"><%= snippet[:html] %></div>
        <pre>
          <code>
<%= escape_html snippet[:html] %>
         </code>
      </pre>
    <% end %>
<% end %>
